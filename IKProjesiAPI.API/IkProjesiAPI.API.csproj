    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>1d990c88-e928-41b1-a426-22c7ac19f8ea</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="6.0.20" />
